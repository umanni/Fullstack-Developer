<div class="container">
  <div class="row justify-content-center">
    <div class="mt-3 ms-3">
      <h3>Lista de Usu√°rios:</h3>
    </div>
  </div>
</div>

<div class="container mt-3">

<table class="table table-hover table-bordered">
  <thead>
    <tr>
      <th>#
      </th>
      <th>Nome</th>
      <th>Email</th>
      <th colspan="3">Administrador</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <% if user != current_user %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.full_name %></td>
        <td><%= user.email %></td>
        <td><%= user.admin %></td>
        <!-- <td><%= link_to "Editar", edit_user_registration_path(user) %></td>
        <td><%= link_to "Deletar", users_backoffice_admin_path(user), method: :delete, data: { confirm: 'Confirmar?'} %></td> -->
      </tr>
    <% end %>
    <% end %>
  </tbody>
  <%= paginate @users %>
</table>

</div>