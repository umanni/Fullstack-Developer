<div class="mt-3 ms-3">
<h3>Lista de Usuários:</h3>
</div>

<div class="container mt-3">

<table class="table table-hover table-bordered">
  <thead>
    <tr>
      <th>#
      </th>
      <th>Nome</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <% if user != current_user %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.full_name %></td>
        <td><%= user.email %></td>
        <td><%= link_to "Editar", edit_user_registration_path(user) %></td>
        <!-- <td><%= link_to "Deletar", users_backoffice_admin_path(user), method: :delete, data: { confirm: 'Confirmar?'} %></td> -->
      </tr>
    <% end %>
    <% end %>
  </tbody>
</table>

</div>
<div class="mt-3 ms-3">
<h3>Importar Usuários</h3>
</div>

<div class="container mt-3">
<div class="input-group mb-3">
    <%= form_tag users_backoffice_admin_import_path, multipart: true do %>
    <%= file_field_tag 'file', class:"form-control", id:"inputGroupFile02" %>
    <%= submit_tag "Importar" %>
    <% end %>
</div>

</div>